 CMake/SlicerCPack.cmake | 2 +-
 1 file changed, 1 insertion(+), 1 deletion(-)

diff --git a/CMake/SlicerCPack.cmake b/CMake/SlicerCPack.cmake
index 16f9133e78..df6e96d150 100644
--- a/CMake/SlicerCPack.cmake
+++ b/CMake/SlicerCPack.cmake
@@ -64,7 +64,7 @@ if(Slicer_BUILD_DICOM_SUPPORT AND NOT Slicer_USE_SYSTEM_DCMTK)
 endif()
 
 # Install Qt designer launcher
-if(Slicer_BUILD_QT_DESIGNER_PLUGINS)
+if(Slicer_BUILD_QT_DESIGNER_PLUGINS AND Slicer_USE_CTKAPPLAUNCHER)
   set(executablename "SlicerDesigner")
   set(build_designer_executable "${QT_BINARY_DIR}/designer${CMAKE_EXECUTABLE_SUFFIX}")
   if(APPLE)
